{
  "manifest_version": 3,
  "name": "Not focus",
  "version": "1.0",
  "description": " does not replace distracting content with inspiration",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "scripting", "activeTab"],
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["main.js", "popup.js"],
          "run_at": "document_start"
      }
  ],
  "action": {
    "default_popup": "index.html"
  }, 
  "host_permissions": ["https://developer.chrome.com/*"]
}

